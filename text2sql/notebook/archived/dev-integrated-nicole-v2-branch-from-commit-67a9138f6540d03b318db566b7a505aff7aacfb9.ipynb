    "#     )\n",
    "#     for x in text_files\n",
    "# ]\n",
    "\n",
    "#----- placeholder\n",
    "text_splitter = RecursiveCharacterTextSplitter(\n",
    "    # Set a really small chunk size, just to show.\n",
    "    chunk_size = 500,\n",
    "    chunk_overlap  = 200,\n",
    "    length_function = len,\n",
    "    is_separator_regex = False,\n",
    ")\n",
    "\n",
    "docs = []\n",
    "for i in range(len(text_files)):\n",
    "    with open(text_files[i], \"r\") as f:\n",
    "        data_add = f.read()\n",
    "    d = text_splitter.create_documents([data_add])\n",
    "    docs += [Document(page_content=\"For Property \" +property_names[i]+ \": \\n\"+e.page_content) for e in d]\n",
    "#----- placeholder\n",
    "    \n",
    "# docs = text_splitter.split_documents(docs)\n",
    "db = FAISS.from_documents(docs, embeddings)\n",
    "db.save_local(\"../code-engine/db-nicole-wip\")\n",
    "db = FAISS.load_local(\"../code-engine/db-nicole-wip\", embeddings)"
   ]
